<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.*?>

<AnchorPane fx:id="rootNode" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.microchip.mcc.foundationservices.modules.softwaredriver.timeoutdriver.TIMEOUTDRIVERViewer">
    <children>
      <GridPane AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
              <TitledPane animated="false" collapsible="false" text="Hardware Settings">
                  <content>
                      <VBox fx:id="subModuleSelectionBox" spacing="5.0" />
                  </content>
              </TitledPane>
            <TitledPane fx:id="examplePane" animated="false" collapsible="false" text="Use Case Examples" GridPane.rowIndex="1">
               <content>
                  <GridPane>
                    <columnConstraints>
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                    </columnConstraints>
                    <rowConstraints>
                      <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                     <children>
                        <Label text="Select use case example to generate:" />
                        <ComboBox fx:id="usecaseComboSettings" onAction="#selectUseCase" GridPane.columnIndex="1" />
                     </children>
                  </GridPane>
               </content>
            </TitledPane>
            <TitledPane animated="false" collapsible="false" text="Use Case Description" GridPane.rowIndex="2">
               <content>
                  <VBox>
                     <children>
                        <Label fx:id="usecaseDescText" />
                     </children>
                  </VBox>
               </content>
            </TitledPane>
         </children>
      </GridPane>
    </children>
    
</AnchorPane>
