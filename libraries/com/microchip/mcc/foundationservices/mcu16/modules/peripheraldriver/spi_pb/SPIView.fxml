<AnchorPane fx:id="rootNode" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.microchip.mcc.foundationservices.mcu16.modules.peripheraldriver.spi_pb.SPIViewer">
    <children>
      <TitledPane animated="false" collapsible="false" text="Hardware Settings" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <content>
            <GridPane hgap="5.0" vgap="5.0">
                <columnConstraints>
                    <ColumnConstraints />
                    <ColumnConstraints maxWidth="149.0" minWidth="130.0" prefWidth="130.0" />
                    <ColumnConstraints />
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints vgrow="NEVER" />
                    <RowConstraints vgrow="NEVER" />
                </rowConstraints>
                <children>
                    <Label text="Default SPI Clock Frequency" />
                    <TextField fx:id="spiClockFrequency" alignment="CENTER" onAction="#onAction" GridPane.columnIndex="1" />
                    <Label text="kHz" GridPane.columnIndex="2" />
                    <Label minHeight="21.0" text="Actual Clock Frequency" GridPane.rowIndex="1" />
                    <Label fx:id="calcSpiClockFrequency" alignment="BOTTOM_LEFT" text="0" textAlignment="CENTER" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                    <Label text="kHz" GridPane.columnIndex="2" GridPane.rowIndex="1" />
                </children>
                <padding>
                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                </padding>
            </GridPane>
         </content>
      </TitledPane>
    </children>
    
</AnchorPane>
