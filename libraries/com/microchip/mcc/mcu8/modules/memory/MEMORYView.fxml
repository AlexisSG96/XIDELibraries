<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.*?>

<AnchorPane fx:id="rootNode" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.microchip.mcc.mcu8.modules.memory.MEMORYViewer">
    <children>
        <GridPane fx:id="gpAll" hgap="5.0" vgap="10.0">
            <columnConstraints>
                <ColumnConstraints />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints vgrow="NEVER" />
            <RowConstraints vgrow="NEVER" />
            </rowConstraints>
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding>
         <children>
            <Label text="Flash Routines are included" />
            <CheckBox fx:id="chkEnableDataEERoutines" mnemonicParsing="false" onAction="#onAction" text="Add DataEE Routines" GridPane.rowIndex="1" />
         </children>
        </GridPane>
    </children>
    
</AnchorPane>
