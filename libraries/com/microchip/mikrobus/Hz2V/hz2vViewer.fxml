<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.*?>

<AnchorPane fx:id="rootNode" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.microchip.mikrobus.Hz2V.hz2vViewer">
    <children>
        <TitledPane animated="false" collapsible="false" layoutY="-2.0" text="Hardware Settings" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <content>
                <VBox>
                    <children>
                        <HBox>
                            <children>
                        <GridPane>
                          <columnConstraints>
                              <ColumnConstraints hgrow="SOMETIMES" />
                            <ColumnConstraints hgrow="SOMETIMES" />
                            <ColumnConstraints hgrow="SOMETIMES" />
                              <ColumnConstraints hgrow="SOMETIMES" />
                          </columnConstraints>
                          <rowConstraints>
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                          </rowConstraints>
                           <children>
                                    <TextField fx:id="VoltText" alignment="CENTER_RIGHT" promptText="Choose Value" GridPane.columnIndex="2" />
                              <Label text="   70 mV ≤ " GridPane.columnIndex="1" />
                                      <Label text="Voltage: " />
                              <Label text=" ≥ 3300 mV " GridPane.columnIndex="3" />
                           </children>
                        </GridPane>
                            </children>
                            <padding>
                                <Insets bottom="20.0" top="20.0" />
                            </padding>    
                        </HBox>
                  <CheckBox fx:id="enablehz" mnemonicParsing="false" onAction="#onAction" text="Enable Hz to V" />
                    </children>
                </VBox>
            </content>
        </TitledPane>
    </children>
    
</AnchorPane>
