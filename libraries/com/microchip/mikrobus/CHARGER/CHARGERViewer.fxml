<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.*?>

<AnchorPane fx:id="rootNode" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.microchip.mikrobus.CHARGER.CHARGERViewer">
    <children>
        <TitledPane animated="false" collapsible="false" layoutY="-2.0" text="Hardware Settings" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <content>
                <VBox>
                    <children>
                        <CheckBox fx:id="enableCHARGER" mnemonicParsing="false" onAction="#onAction" text="Enable CHARGER" />
                        <HBox>
                            <children>
                                <Label text="Select Item:  " />
                                <ComboBox fx:id="comboSettings" prefWidth="150.0" />
                            </children>
                            <padding>
                                <Insets bottom="20.0" top="20.0" />
                            </padding>    
                        </HBox>
                        <TextField fx:id="textSettings" promptText="Choose Value" />
                    </children>
                </VBox>
            </content>
        </TitledPane>
    </children>
    
</AnchorPane>